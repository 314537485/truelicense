<p:properties
        name="Disable License Wizard"
        xml:lang="en"
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:p="${project.url}/xml/archetype-properties">
    <p:annotation>
        <p:documentation>
            This example adds
            <code>disableLicenseWizard=true</code>
            to the Basic Configuration example in order to completely remove
            the license wizard dialog from the generated code.
        </p:documentation>
    </p:annotation>
    <p:list>
        <p:artifactId>disableLicenseWizard</p:artifactId>
        <p:companyName>Company Inc.</p:companyName>
        <p:disableLicenseWizard>true</p:disableLicenseWizard>
        <p:groupId>com.company.product</p:groupId>
        <p:licensingSubject>Product 1</p:licensingSubject>
        <p:password>test1234</p:password>
        <p:version>1.0-SNAPSHOT</p:version>
    </p:list>
</p:properties>
