<p:properties
        name="Hide Uninstall Action"
        xml:lang="en"
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:p="${project.url}/xml/archetype-properties">
    <p:documentation>
        This example adds
        <code>hideUninstallAction=true</code>
        to the Basic Configuration example in order to remove the
        <i>Uninstall ${dollar}{licensingSubject} license key</i>
        action from the license wizard dialog.
    </p:documentation>
    <p:list>
        <p:artifactId>hideUninstallAction</p:artifactId>
        <p:companyName>Company Inc.</p:companyName>
        <p:groupId>com.company.product</p:groupId>
        <p:hideUninstallAction>true</p:hideUninstallAction>
        <p:licensingSubject>Product 1</p:licensingSubject>
        <p:password>test1234</p:password>
        <p:version>1.0-SNAPSHOT</p:version>
    </p:list>
</p:properties>
