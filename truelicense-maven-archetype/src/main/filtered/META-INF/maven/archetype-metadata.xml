<archetype-descriptor
        xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd">
    <requiredProperties/>
    <fileSets>
        <fileSet>
            <directory/>
            <includes>
                <include>.*ignore</include>
            </includes>
        </fileSet>
    </fileSets>
    <modules>
        <module dir="__rootArtifactId__-keygen"
                id="${dollar}{rootArtifactId}-keygen"
                name="${dollar}{rootArtifactId}-keygen">
            <fileSets>
                <fileSet packaged="true" filtered="true">
                    <directory>src/main/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/main/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
                <fileSet packaged="true" filtered="true">
                    <directory>src/main/filtered</directory>
                </fileSet>
                <fileSet packaged="true" filtered="true">
                    <directory>src/test/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/test/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
            </fileSets>
        </module>
        <module dir="__rootArtifactId__-keymgr"
                id="${dollar}{rootArtifactId}-keymgr"
                name="${dollar}{rootArtifactId}-keymgr">
            <fileSets>
                <fileSet packaged="true" filtered="true">
                    <directory>src/main/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/main/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
                <fileSet packaged="true" filtered="true">
                    <directory>src/main/filtered</directory>
                </fileSet>
                <fileSet packaged="true" filtered="true">
                    <directory>src/test/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/test/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
                <fileSet>
                    <directory>src/test/resources</directory>
                </fileSet>
            </fileSets>
        </module>
        <module dir="__rootArtifactId__-keymgrsvc"
                id="${dollar}{rootArtifactId}-keymgrsvc"
                name="${dollar}{rootArtifactId}-keymgrsvc">
            <fileSets>
                <fileSet packaged="true" filtered="true">
                    <directory>src/main/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/main/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
                <fileSet packaged="true" filtered="true">
                    <directory>src/test/java</directory>
                    <excludes>
                        <exclude>**/*.vtl</exclude>
                    </excludes>
                </fileSet>
                <fileSet packaged="true">
                    <directory>src/test/java</directory>
                    <includes>
                        <include>**/*.vtl</include>
                    </includes>
                </fileSet>
            </fileSets>
        </module>
    </modules>
</archetype-descriptor>
